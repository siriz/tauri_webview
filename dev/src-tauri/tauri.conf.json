{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "TauriWebview",
  "version": "0.1.0",
  "identifier": "com.siriz.tauriwebview",
  "build": {
    "devUrl": "http://localhost:5173",
    "frontendDist": "../../../build/dist/html"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "TauriWebview",
        "width": 800,
        "height": 600,
        "url": "http://localhost:8000"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": false,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
