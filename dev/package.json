{
  "name": "tauriwebview",
  "private": true,
  "version": "0.2.16",
  "type": "module",
  "scripts": {
    "predev": "node scripts/copy-contents.js",
    "dev": "cd src-tauri && tauri dev",
    "prebuild": "node scripts/update-version.js && node scripts/copy-contents.js",
    "build": "cd src-tauri && tauri build && cd .. && node scripts/create-dist.js",
    "postbuild": "node scripts/create-release-zip.js",
    "release": "npm run build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.0.0",
    "@tauri-apps/plugin-opener": "^2.0.0"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  }
}
